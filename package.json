{
  "name": "workshop-storybook-signin-final",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "storybook-ci": "start-storybook -p 6006 -s public --ci",
    "build-storybook": "build-storybook -s public && npx sb extract",
    "backstop:pre-test": "rimraf backstop_output && npm run build-storybook",
    "backstop:test": "npm run backstop:pre-test && backstop test --config=backstop.config.js",
    "backstop:approve": "backstop approve --config=backstop.config.js",
    "loki:test": "loki test",
    "loki:test-ci": "build-storybook && loki --requireReference --reactUri file:./storybook-static",
    "loki:approve": "loki approve"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/addon-storyshots": "^6.0.21",
    "@storybook/node-logger": "^6.0.21",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.21",
    "babel-loader": "^8.1.0",
    "backstopjs": "^5.3.2",
    "chromatic": "^5.2.0",
    "loki": "^0.28.1",
    "react-is": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "start-server-and-test": "^1.12.1"
  }
}
